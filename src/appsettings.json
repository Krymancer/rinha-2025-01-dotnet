{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Postgres": "server=db; database=db; user id=user; password=password; pooling=true; maximum pool size=20; multiplexing=true; no reset on close=true; max auto prepare=20; auto prepare min usages=1;"
  },
  "PaymentProcessors": {
    "Default": {
      "BaseUrl": "http://payment-processor-default:8080"
    },
    "Fallback": {
      "BaseUrl": "http://payment-processor-fallback:8080"
    }
  },
  "Jobs": {
    "ProcessingBatchSize": 100,
    "ProcessingParalellism": 10,
    "ProcessingWait": 40,
    "PersistenceBatchSize": 200,
    "PersistenceWait": 20
  }
}